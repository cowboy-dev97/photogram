<% if post.errors.any? %>
  <h2><%= pluralize post.errors.count, "error" %> errors</h2>
  <ul>
    <% post.errors.each do |error|%>
      <li><%= error.full_message %></li>
    <% end %>
  </ul>
<% end %>
<div class="flex justify-between">
  <div class="flex items-center">
    <p class="cursor-pointer hover:underline"><%= post.user.user_name %></p>
    <span class="px-1">&#183;</span>
    <p class="text-blue-500 hover:text-blue-300 cursor-pointer">Follow</p>
  </div>
  <div class="flex gap-2">
    <% if post.user.id == current_user&.id %>
      <%= link_to "Edit post", edit_post_path(post.id), class:"text-blue-500 hover:text-blue-300 cursor-pointer" %>
    <% end%>
    <% if action_name == "show" %>
      <%= link_to "Back", root_path, class:"text-blue-500 hover:text-blue-300 cursor-pointer"%>
    <% end %>
  </div>
</div>
<div class="w-full hover:opacity-75 shadow-lg">
  <%= link_to post do %>
    <%= image_tag post.image, class:"rounded-sm object-cover h-[500px] w-full" %>
  <% end %>
</div>
<div class="flex justify-between py-2">
  <p><strong><%= post.user.user_name %></strong> <span class="italic"><%= post.caption %></span></p>
